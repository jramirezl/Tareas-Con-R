{
  "name": "R with Conda and Jupyter",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "terminal.integrated.defaultProfile.linux": "bash"
  },
  "postCreateCommand": "echo 'Container listo con R y Conda'",
  "forwardPorts": [8888],
  "customizations": {
    "vscode": {
      "extensions": [
        "REditorSupport.r",
        "Ikuyadeu.r",
        "ms-toolsai.jupyter",
        "ms-python.python"
      ]
    }
  },
  "features": {}
}
